{
  "name": "kookjein-react-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-oauth/google": "^0.10.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "aws-sdk": "^2.1362.0",
    "axios": "^1.4.0",
    "browser-image-compression": "^2.0.2",
    "draft-js": "^0.11.7",
    "firebase": "^9.21.0",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-chat-elements": "^12.0.10",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-dropdown": "^1.11.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.7.1",
    "react-linkify": "^1.0.0-alpha",
    "react-modal": "^3.16.1",
    "react-modern-drawer": "^1.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0",
    "react-tag-input": "^6.8.1",
    "react-toastify": "^9.1.3",
    "react-type-animation": "^3.1.0",
    "redux": "^4.2.1",
    "styled-components": "^5.3.11",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "winBuild": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "aws s3 sync ./build s3://www.kookjein.com",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E1KU9A9Y5CW7W2 --paths / /index.html /error.html /manifest.json /favicon.ico"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--disable-web-security"
    ],
    "include": [
      "/",
      "/service/company",
      "/service/developer",
      "/auth/login",
      "/auth/signup",
      "/browse",
      "/browse-jobs",
      "/jobs/:jobId",
      "/user/:userId",
      "/company/:companyId",
      "/post-job/flow-1"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4"
  }
}
